{
  "headers": [
    {
      "source": "/Build/(.*).js.gz",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "gzip"
        },
        {
          "key": "Content-Type",
          "value": "text/javascript"
        }
      ]
    },
    {
      "source": "/Build/(.*).wasm.gz",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "gzip"
        },
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/Build/UnityLoader.js", "destination": "/Build/UnityLoader.js" }
  ]
}

